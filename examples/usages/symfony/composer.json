{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./../../../src/Integration/Symfony/Blade",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./../../../src/Integration/Symfony/Handlebars",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./../../../src/Integration/Symfony/Latte",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./../../../src/Integration/Symfony/Mustache",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./../../../src/Integration/Symfony/Plates",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./../../../src/Integration/Symfony/Smarty",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./../../../src/Integration/Symfony/Twig",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.13",
        "schranz-templating/symfony-blade-integration": "@dev",
        "schranz-templating/symfony-handlebars-integration": "@dev",
        "schranz-templating/symfony-latte-integration": "@dev",
        "schranz-templating/symfony-mustache-integration": "@dev",
        "schranz-templating/symfony-plates-integration": "@dev",
        "schranz-templating/symfony-smarty-integration": "@dev",
        "schranz-templating/symfony-twig-integration": "@dev",
        "symfony/console": "6.1.*",
        "symfony/dotenv": "6.1.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.1.*",
        "symfony/runtime": "6.1.*",
        "symfony/yaml": "6.1.*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.1.*"
        }
    }
}
