{
    "name": "schranz-templating/symfony-blade-integration",
    "description": "A integration of template renderer into symfony via blade template engine.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Schranz\\Templating\\Integration\\Symfony\\Blade\\": ""
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./../../../TemplateRenderer",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./../../../Adapter/Blade",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "illuminate/view": "^8.0 || ^9.0",
        "schranz-templating/blade-adapter": "^0.1",
        "symfony/config": "^4.4 || ^5.4 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
        "symfony/http-kernel": "^4.4 || ^5.4 || ^6.0"
    },
    "minimum-stability": "dev"
}
