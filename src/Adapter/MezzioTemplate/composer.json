{
    "name": "schranz-templating/mezzio-template-adapter",
    "description": "An adapter for the template renderer for mezzio template engine.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Schranz\\Templating\\Adapter\\MezzioTemplate\\": ""
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./../../TemplateRenderer",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "mezzio/mezzio-template": "^1.0 || ^2.0",
        "schranz-templating/template-renderer": "^0.1"
    },
    "minimum-stability": "dev"
}
